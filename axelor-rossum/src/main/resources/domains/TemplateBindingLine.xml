<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_5.3.xsd">

   <module package="com.axelor.apps.rossum.db" name="rossum"/>
  
   <entity name="TemplateBindingLine">
      <many-to-one name="templateBinding" ref="com.axelor.apps.rossum.db.TemplateBinding" title="Template binding"/>
      <many-to-one name="metaField" ref="com.axelor.meta.db.MetaField" title="Field"/>
      
      <string name="rossumField" title="Rossum field" readonly="true" initParam="true"/>
      <string name="labelField" title="Label field" readonly="true" initParam="true"/>
      <string name="metaFieldSelection" title="Meta field selection" readonly="true" initParam="true"/>
   </entity>

</domain-models>
